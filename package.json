{
  "name": "chrome-pdiff",
  "version": "0.0.1",
  "description": "get pdiffs for elements using the Chrome Remote Protocol Viewer",
  "main": "index.js",
  "scripts": {
    "inspect": "chrome-remote-interface inspect",
    "chrome": "/Applications/Google\\ Chrome\\ Canary.app/Contents/MacOS/Google\\ Chrome\\ Canary --remote-debugging-port=9222 http://localhost:9222",
    "start": "babel-node index.js",
    "lint": "eslint . || true",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "di": "node-inspector -d=7000 -p=8000 --save-live-edit --hidden node_modules/",
    "ds1": "babel ./src -d ./build/server/src -s -w; babel index.js -d ./build/server -s -w",
    "ds2": "nodemon --watch ./build/server --delay 100ms --debug=7000 ./build/server/index.js"
  },
  "keywords": [],
  "author": "Sarah Lim <sarah@sarahlim.com> (http://sarahlim.com)",
  "license": "ISC",
  "dependencies": {
    "chrome-remote-interface": "^0.14.3",
    "co": "^4.6.0",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.0.0",
    "shortcss": "^0.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.17.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-flowtype": "^2.4.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "flow-bin": "^0.30.0"
  }
}
